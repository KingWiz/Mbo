
@{
    ViewBag.Title = "Create";
}
<style type="text/css">
    .prashil{
        background-color:indigo;
        height:120px;
        color:white;
    }
    .aryal{
        padding-top:20px;
    }
    .submitButton{
        color:indigo;
    }
</style>

<h2>Create</h2>
<div class="prashil">
    <form class="aryal" action="SaveData">
        <label class="col-md-2">Name</label>
        <div class="col-md-4">
            <input type="text" name="empName" class="form-control" />
        </div><br/>
        <label class="col-md-2">Address</label>
        <div class="col-md-4">
            <input type="text" name="empAddress" class="form-control" />
        </div><br/>
        <label class="col-md-2">Contact</label>
        <div class="col-md-4">
            <input type="text" name="empContact" class="form-control" />
        </div><br/>
        <input type="submit" name="Submit" class="submitButton"/>
    </form>
</div>

