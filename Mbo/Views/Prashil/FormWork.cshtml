@model List<Mbo.Models.Employee>
@{
    ViewBag.Title = "index";
    int sn = 0;
}
<style>
    .empTable{
        background-color: gainsboro;
        width: 20%;
    }
</style>
<a href="@Url.Action("Create")">Create</a><br />
<a href="@Url.Action("esdCreate", "empSalaryDetails")">Create esdDetails</a>

<h2>Services</h2>
<h2>Table of the day mbo</h2>

<table border="1" class="empTable">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Address</th>
        <th>Contact</th>
        <th>Action</th>
    </tr>
    @foreach(var data in Model)
    {
    <tr>
        <td>@(data.empID)</td>
        <td>@(data.empName)</td>
        <td>@(data.empAddress)</td>
        <td>@(data.empContact)</td>
        <td><a href="@Url.Action("Edit", new {empID=data.empID})">Edit</a></td>

    </tr>

    }

</table>
<br/>
<butoon class="btn btn-success"><a href="@Url.Action("Create")">Add record</a></butoon>

